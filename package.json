{
  "name": "holidia",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "start:development": "NODE_ENV=development expo start --dev-client",
    "start:production": "NODE_ENV=production expo start --dev-client",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "prepare": "husky install"
  },
  "dependencies": {
    "@dev-plugins/react-query": "^0.2.0",
    "@expo/vector-icons": "^14.0.0",
    "@gorhom/bottom-sheet": "^5.1.2",
    "@marceloterreiro/flash-calendar": "^1.3.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-navigation/native": "^7.0.3",
    "@shopify/flash-list": "1.7.3",
    "@shopify/react-native-skia": "1.5.0",
    "@tanstack/react-query": "^5.74.4",
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "expo": "^52.0.37",
    "expo-blur": "^14.0.3",
    "expo-constants": "~17.0.7",
    "expo-dev-client": "~5.0.19",
    "expo-dev-launcher": "^5.0.17",
    "expo-image": "~2.0.6",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.6",
    "expo-squircle-view": "^1.1.0",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "nanoid": "^5.1.5",
    "nativewind": "latest",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-flexible-grid": "^0.2.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-image-colors": "^2.4.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-reanimated-carousel": "^4.0.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-squircle": "^1.1.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.10",
    "strip-ansi": "^6.0.1",
    "tailwind-merge": "^3.0.2",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "latest",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "husky": "^9.1.7",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "private": true,
  "resolutions": {
    "strip-ansi": "6.0.1"
  }
